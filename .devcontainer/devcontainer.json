{
  "image": "chef/chefworkstation",
  "containerEnv": {
    "CHEF_LICENSE": "accept",
    "HAB_LICENSE": "accept"
  },
  "extensions": [
    "chef-software.chef"
  ],
  "settings": {
    "extensions.ignoreRecommendations": true,
    "terminal.integrated.profiles.linux": {
      "default": {
        "path": "/bin/bash"
      }
    },
    "terminal.integrated.defaultProfile.linux": "default"
  }

}